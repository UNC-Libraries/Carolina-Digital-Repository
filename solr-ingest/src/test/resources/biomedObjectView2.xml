<foxml:datastream ID="MD_DESCRIPTIVE" STATE="A" CONTROL_GROUP="X" VERSIONABLE="true">
<foxml:datastreamVersion ID="MD_DESCRIPTIVE.0" LABEL="Descriptive Metadata" CREATED="2012-10-12T17:15:39.251Z" MIMETYPE="text/xml" SIZE="9540">

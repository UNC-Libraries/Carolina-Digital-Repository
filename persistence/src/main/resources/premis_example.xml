    <!-- At present there are two kinds of PREMIS data that are available directly from the CDR.  The first kind of 
        PREMIS elements available below describe our objects and their relationships.  The information about all of
        these objects is recorded in Fedora Object XML and the iRODS catalog.  See the storage notes below for details. -->
    <!-- Each semantic grouping will be annotated in the following fashion: -->
    <!-- ORIGIN: 
         STORAGE:  
         NOTE:    
    -->

    <!--  ORIGIN: DDA plugin METS - each representation begins as a mets:div element (or a single object submission) 
         STORAGE: Fedora Object - a PREMIS representation object maps to a single Fedora Object
        WORKFLOW: The DDA plugin creates a METS div for each folder and file.  
                  The ingest process transforms METS divs into Fedora objects, each represented by a FOXML doc. -->
    <object xsi:type="representation">


        <!-- ORIGIN: Ingest - A PID is generated by ingest code.
            STORAGE: Fedora Object PID - PID is a property of every Fedora Object.
           WORKFLOW: The ingest code uses a UUID algorithym to generate a universally unique PID for each Fedora Object. -->
        <objectIdentifier>
            <objectIdentifierType>Fedora PID</objectIdentifierType>
            <objectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5</objectIdentifierValue>
        </objectIdentifier>

        <!-- ORIGIN: DDA plugin 
            STORAGE: Fedora Object label (also MODS:title)
           WORKFLOW: The DDA plugin record the file/folder name as a MODS title.  The ingest code sets the Fedora Object label
                     to the content of MODS:title.  -->
        <originalName>File - untyped with ORDER 5</originalName>
        
        <!-- Listed below are the relationships with other PREMIS objects.  Right now these others do not include 
             first-class Fedora Objects. These are data streams that are part of a Fedora object.  
             All these relationships with data streams are stored in the same way. -->
        <!-- ORIGIN: see notes below on "file" and "bitstream" type PREMIS objects
            STORAGE: FOXML Datastream element (indexed and queried via the Fedora Resource Index hasDatastream triple)
           WORKFLOW: see notes below on "file" and "bitstream" type PREMIS objects  -->
        <relationship>
            <relationshipType>has part</relationshipType>
            <relationshipSubType>fedora-system:hasDatastream</relationshipSubType>
            <relatedObjectIdentification>
                <relatedObjectIdentifierType>Fedora Datastream PID</relatedObjectIdentifierType>
                <relatedObjectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+RELS-EXT+RELS-EXT.0</relatedObjectIdentifierValue>
            </relatedObjectIdentification>
        </relationship>
        <relationship>
            <relationshipType>has part</relationshipType>
            <relationshipSubType>fedora-system:hasDatastream</relationshipSubType>
            <relatedObjectIdentification>
                <relatedObjectIdentifierType>Fedora Datastream PID</relatedObjectIdentifierType>
                <relatedObjectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+MD_DESCRIPTIVE+MD_DESCRIPTIVE.0</relatedObjectIdentifierValue>
            </relatedObjectIdentification>
        </relationship>
        <relationship>
            <relationshipType>has part</relationshipType>
            <relationshipSubType>fedora-system:hasDatastream</relationshipSubType>
            <relatedObjectIdentification>
                <relatedObjectIdentifierType>Fedora Datastream PID</relatedObjectIdentifierType>
                <relatedObjectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+DATA_FILE+DATA_FILE.0</relatedObjectIdentifierValue>
            </relatedObjectIdentification>
        </relationship>
        <relationship>
            <relationshipType>has part</relationshipType>
            <relationshipSubType>fedora-system:hasDatastream</relationshipSubType>
            <relatedObjectIdentification>
                <relatedObjectIdentifierType>Fedora Datastream PID</relatedObjectIdentifierType>
                <relatedObjectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+DC+DC.0</relatedObjectIdentifierValue>
            </relatedObjectIdentification>
        </relationship>
        <relationship>
            <relationshipType>has part</relationshipType>
            <relationshipSubType>fedora-system:hasDatastream</relationshipSubType>
            <relatedObjectIdentification>
                <relatedObjectIdentifierType>Fedora Datastream PID</relatedObjectIdentifierType>
                <relatedObjectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+MD_EVENTS+MD_EVENTS.1</relatedObjectIdentifierValue>
            </relatedObjectIdentification>
        </relationship>
    </object>


    <!-- This is a standard Fedora XML datastream for recording object-to-object relationships in RDF. -->
    <!-- ORIGIN: Ingest
        STORAGE: metadata about the datastream is in Fedora FOXML
       WORKFLOW: The RELS-EXT datastream is added during ingest processing to record a variety of object properties and 
                 relationships, including the "contains" relationship, content models, owner, etc.. -->
    <object xsi:type="bitstream">
        <objectIdentifier>
            <!-- The datastream ID is "RELS-EXT", a Fedora standard. -->
            <objectIdentifierType>Fedora Datastream PID</objectIdentifierType>
            <objectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+RELS-EXT+RELS-EXT.0</objectIdentifierValue>
        </objectIdentifier>
        <objectCharacteristics>
            
            <!-- Composition level is 1 because this datastream is inline XML in a larger FOXML file. -->
            <!-- ORIGIN: Ingest; STORAGE: Fedora datastream type property; WORKFLOW: standard Fedora datastream -->
            <compositionLevel>1</compositionLevel>
            
            <!-- ORIGIN: Fedora; STORAGE: FOXML; WORKFLOW: Current size of the bitstream, taken from Fedora API. -->
            <size>1306</size>
            
            <!-- ORIGIN: Ingest; STORAGE: FOXML; WORKFLOW: This format is Fedora standard for RELS-EXT. -->
            <format>
                <formatDesignation>
                    <formatName>text/xml</formatName>
                </formatDesignation>
                <formatNote>IANA MIME-type</formatNote>
            </format>
            
        </objectCharacteristics>
        
        
        <!-- A storage element is listed here for each replica on record in the iRODS catalog. -->
        <!-- ORIGIN: iRODS PUT operation; STORAGE: iRODS ICAT; WORKFLOW: queried via jargon (iRODS java client) -->
        <storage>
            <contentLocation>
                <contentLocationType>iRODS URI</contentLocationType>
                <!-- note that this is the FOXML file in which this data is embedded. -->
                <contentLocationValue>irods://ono-sendai.dhcp.unc.edu:1247/cdrZone/home/fedora/objects/2010/0115/11/22/uuid_2fadcb3b-3b25-4397-b9c0-7938ed5eecf5</contentLocationValue>
            </contentLocation>
            <!-- These are the resource class and type for the replica, drawn from iRODS ICAT. -->
            <storageMedium>archive - unix file system</storageMedium>
        </storage>
    </object>


    <!-- ORIGIN: DDA plugin produces MODS for each folder and file
        STORAGE: metadata about this datastream and the data itself are in Fedora FOXML
       WORKFLOW: The MODS record is made during DDA plugin processing from a variety of sources, including Droid and Java
        I/O.  It is placed in this particular Fedora object datastream by ingest processing. -->
    <object xsi:type="bitstream">
        <!-- The datastream ID for MODS is always "MD_DESCRIPTIVE", a CDR standard. -->
        <objectIdentifier>
            <objectIdentifierType>Fedora Datastream PID</objectIdentifierType>
            <objectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+MD_DESCRIPTIVE+MD_DESCRIPTIVE.0</objectIdentifierValue>
        </objectIdentifier>
        <objectCharacteristics>
            
            <!-- Composition level is 1 because this datastream is inline XML in a larger FOXML file. -->
            <!-- ORIGIN: Ingest; STORAGE: Fedora datastream type property; WORKFLOW: standard Fedora datastream -->
            <compositionLevel>1</compositionLevel>
            
            <!-- ORIGIN: Fedora; STORAGE: FOXML; WORKFLOW: Current size of the bitstream, taken from Fedora API. -->
            <size>1599</size>
            
            <!-- ORIGIN: DDA plugin; STORAGE: FOXML; WORKFLOW: This format is CDR standard for descriptive metadata. -->
            <format>
                <formatDesignation>
                    <!-- format here was originally placed in FOXML by ingest code -->
                    <formatName>text/xml</formatName>
                </formatDesignation>
                <formatNote>IANA MIME-type</formatNote>
            </format>
        </objectCharacteristics>
        
        <!-- A storage element is listed here for each replica on record in the iRODS catalog. -->
        <!-- ORIGIN: iRODS PUT operation; STORAGE: iRODS ICAT; WORKFLOW: queried via jargon (iRODS java client) -->
        <storage>
            <contentLocation>
                <contentLocationType>iRODS URI</contentLocationType>
                <!-- note that this is the FOXML file in which this data is embedded. -->
                <contentLocationValue>irods://ono-sendai.dhcp.unc.edu:1247/cdrZone/home/fedora/objects/2010/0115/11/22/uuid_2fadcb3b-3b25-4397-b9c0-7938ed5eecf5</contentLocationValue>
            </contentLocation>
            <storageMedium>archive - unix file system</storageMedium>
        </storage>
    </object>


    <!-- This object describes the Fedora datastream which holds a source data file, i.e. a submitted digital object. 
         ORIGIN: The DDA itself is the first to recognize the existence of each data file.
        STORAGE: Metadata about this datastream is stored in Fedora FOXML.  The data is stored and replicated in a separate iRODS files.
        WORKFLOW: The DDA passes information about the file to the DDA plugin, which gathers more information via Java and
                  records it all in METS.  Ingest processing moves the data record to a FOXML datastream and sends the
                  data and the FOXML to Fedora.  Fedora stores the FOXML and the data file in iRODS.  IRODS replicates
                  the FOXML and the data file to an archival tape system.  -->
    <object xsi:type="file">
        <!-- This is the main source data for a file object. -->
        <objectIdentifier>
            <objectIdentifierType>Fedora Datastream PID</objectIdentifierType>
            
            <!-- The datastream ID for the data file is "DATA_FILE", part of the CDR "File" content model. -->
            <objectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+DATA_FILE+DATA_FILE.0</objectIdentifierValue>
        </objectIdentifier>
        <objectCharacteristics>
            
            <!-- This file is stored independently and not embedded. -->
            <!-- ORIGIN: Ingest; STORAGE: Fedora datastream type property; WORKFLOW: standard Fedora managed datastream -->
            <compositionLevel>0</compositionLevel>
            
            <fixity>
                <!-- This digest will match the earliest known digest for the file.  In the case of DDA
                processing, this digest will match a checksum generated by the DDA code. If a file was
                submitted without a checksum, then this will match a checksum generated by ingest code.  The
                digest number reported below is queried from the iRODS ICAT.  -->
                <!-- ORIGIN: Droid 
                    STORAGE: Fedora datastream checksum property 
                   WORKFLOW: The DDA runs Droid, which produces the file checksum.  
                    It is passed to the DDA plugin, which record it in METS.  
                    Ingest, Fedora, irodsfedora module and iRODS itself verify the checksum after every transport. -->
                <messageDigestAlgorithm>MD5</messageDigestAlgorithm>
                <messageDigest>47025973a293d2dcc58eb9ad9c355070</messageDigest>
                <messageDigestOriginator>IRODS Microservice</messageDigestOriginator>
            </fixity>
            
            <!-- size as reported by the Fedora API -->
            <size>230571</size>
            
            <format>
                <formatDesignation>
                    <!-- This format comes from Fedora.  In the DDA scenario, it comes from DROID. In 
                    the case of single file ingest, it comes from Spring Framework mime-type detection
                    upon upload. -->
                    <!-- ORIGIN: Droid
                        STORAGE: Fedora datastream MIMETYPE property
                       WORKFLOW: DDA Plugin records mimetype from Droid in METS.  Ingest code puts that in FOXML.  -->
                    <formatName>application/pdf</formatName>
                </formatDesignation>
                <!-- this note is hardcoded for all source data files, since we have no validation at this time. -->
                <formatNote>unvalidated IANA MIME-type</formatNote>
            </format>
        </objectCharacteristics>
        
        <!-- The DDA plugin puts this value in MODS title. It becomes the Fedora datastream label property and we retrieve it via the Fedora API. -->
        <!-- ORIGIN: Ingest; STORAGE: Fedora datastream label property; WORKFLOW: standard Fedora managed datastream -->
        <originalName>tutorial10_overview_19june2007.pdf</originalName>
        
        <!-- A storage element is listed here for each replica on record in the iRODS catalog. -->
        <!-- ORIGIN: iRODS PUT operation; STORAGE: iRODS ICAT; WORKFLOW: queried via jargon (iRODS java client) -->
        <storage>
            <contentLocation>
                <contentLocationType>iRODS URI</contentLocationType>
                <contentLocationValue>irods://ono-sendai.dhcp.unc.edu:1247/cdrZone/home/fedora/datastreams/2010/0115/11/22/uuid_2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+DATA_FILE+DATA_FILE.0</contentLocationValue>
            </contentLocation>
            <storageMedium>archive - unix file system</storageMedium>
        </storage>
    </object>


    <object xsi:type="bitstream">
        <!-- This is the Dublin Core XML, which is crosswalked from MODS.  In DDA scenarios the MODS is
        produced by a combination of DROID and the plugin (Java I/O). -->
        <objectIdentifier>
            <objectIdentifierType>Fedora Datastream PID</objectIdentifierType>
            <objectIdentifierValue>uuid:2fadcb3b-3b25-4397-b9c0-7938ed5eecf5+DC+DC.0</objectIdentifierValue>
        </objectIdentifier>
        <objectCharacteristics>
            <!-- embedded in FOXML file -->
            <compositionLevel>1</compositionLevel>
            <!-- set by Fedora upon ingest/update -->
            <size>1005</size>
            <format>
                <!-- set by ingest code when crosswalked -->
                <formatDesignation>
                    <formatName>text/xml</formatName>
                </formatDesignation>
                <formatNote>IANA MIME-type</formatNote>
            </format>
        </objectCharacteristics>
        <storage>
            <contentLocation>
                <contentLocationType>iRODS URI</contentLocationType>

	<foxml:datastream ID="WSDL" CONTROL_GROUP="X" STATE="A" VERSIONABLE="true">
		<foxml:datastreamVersion
			FORMAT_URI="http://schemas.xmlsoap.org/wsdl/" ID="WSDL1.0" LABEL="WSDL Bindings"
